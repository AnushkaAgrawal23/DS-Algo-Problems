    "DISTRIBUTE CANDIES"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 5,
   "metadata": {},
   "outputs": [],
   "source": [
    "def distribute(arr):\n",
    "    n = len(arr)\n",
    "    s = set(arr)\n",
    "    candies = min(len(s),n//2)\n",
    "    return candies"
   ]
  },
