   "source": [
    "# Find the number of trailing zeros in n!\n",
    "def zeros(num):\n",
    "    count = 0\n",
    "    i = 5\n",
    "    while(num/i>=1):\n",
    "        count += int(num/i)\n",
    "        i *= 5\n",
    "    return count"
   ]
  },
